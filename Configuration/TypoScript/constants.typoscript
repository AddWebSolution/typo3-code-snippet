##############################
#### FLUID STYLED CONTENT ####
##############################
styles {
    templates {
        layoutRootPath = EXT:so_typo3/Resources/Private/Layouts/ContentElements/
        partialRootPath = EXT:so_typo3/Resources/Private/Partials/ContentElements/
        templateRootPath = EXT:so_typo3/Resources/Private/Templates/ContentElements/
    }
}


############
### PAGE ###
############
page {
     fluidtemplate {
        layoutRootPath = EXT:so_typo3/Resources/Private/Layouts/Page/
        partialRootPath = EXT:so_typo3/Resources/Private/Partials/Page/
        templateRootPath = EXT:so_typo3/Resources/Private/Templates/Page/
    }
    meta {
        description =
        author =
        keywords =
        viewport = width=device-width, initial-scale=1
        robots = index,follow
        apple-mobile-web-app-capable = no
        compatible = IE=edge
    }
    tracking {
        google {
            trackingID =
            anonymizeIp = 1
        }
    }
}


##############
### CONFIG ###
##############
config {
    no_cache = 0
    removeDefaultJS = 0
    admPanel = 1
    prefixLocalAnchors = all
    headerComment = build by sitepackagebuilder.com
    sendCacheHeaders = 1
}


# Define a constant for the path to the image
# TypoScript setup
lib.myImage = IMAGE
lib.myImage {
  file = EXT:so_typo3/Resources/Public/Star.svg
  altText = Tags Icon
}


